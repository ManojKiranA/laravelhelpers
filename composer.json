{
    "name": "manojkiran/laravelhelpers",
    "description": "Helpers for The Laravel",
    "keywords": [
        "manojkiran",
        "laravelhelpers"
    ],
    "homepage": "https://github.com/manojkiran/laravelhelpers",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Manojkiran.A",
            "email": "manojkiran10031998@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Manojkiran\\LaravelHelpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Manojkiran\\LaravelHelpers\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Manojkiran\\LaravelHelpers\\LaravelHelpersServiceProvider"
            ],
            "aliases": {
                "LaravelHelpers": "Manojkiran\\LaravelHelpers\\LaravelHelpersFacade"
            }
        }
    }
}
